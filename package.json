{
  "name": "@scientisst/sense-apps",
  "version": "3.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "package": "pnpm run -r package",
    "test:unit": "pnpm run -r test:unit",
    "test:integration": "pnpm run -r test:integration",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "package:ui": "pnpm run --filter @scientisst/sense-ui package",
    "storybook": "pnpm run package:ui && concurrently -n '@scientisst/sense-ui,@scientisst/storybook' 'pnpm run package:ui' 'pnpm run --filter @scientisst/storybook storybook'"
  },
  "dependencies": {
    "@material/material-color-utilities": "0.2.7",
    "tailwind-merge": "2.2.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@playwright/test": "1.40.0",
    "@sveltejs/adapter-static": "3.0.1",
    "@sveltejs/kit": "2.5.5",
    "@sveltejs/package": "2.3.1",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.12",
    "@tauri-apps/api": "2.0.0-beta.6",
    "@tauri-apps/cli": "2.0.0-beta.11",
    "@tauri-apps/plugin-shell": "2.0.0-beta.2",
    "@types/eslint": "8.56.7",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-svelte": "2.35.1",
    "eslint": "8.57.0",
    "postcss": "8.4.38",
    "prettier-plugin-svelte": "3.2.2",
    "prettier-plugin-tailwindcss": "0.5.13",
    "prettier": "3.2.5",
    "publint": "0.2.7",
    "svelte-check": "3.6.9",
    "svelte": "4.2.12",
    "tailwindcss": "3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.4",
    "vite": "5.2.8",
    "vitest": "1.4.0"
  }
}
